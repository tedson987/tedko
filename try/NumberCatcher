using System;

namespace NumberCatcher
{
    class Program
    {
        static void Main(string[] args)
        {
            int input; 
            try
            {
                input = int.Parse(Console.ReadLine());
                Console.WriteLine("Its a nnumber");
            }
            catch (Exception)
            {
                Console.WriteLine("Invalid input!");
                
            }
            
        }
        
    }
}
