using System;

namespace ExchangeVariables
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = int.Parse(Console.ReadLine());
            int b = int.Parse(Console.ReadLine());
            int c = a;

            a = b;
            b = c;
            Console.WriteLine($"Before: \r\n a = {a}\r\n b = {b}");
            Console.WriteLine($"After: \r\n a = {a}\r\n b = {b}");



        }
    }
}
